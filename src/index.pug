doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")

    title= title
    meta(http-equiv="Content-type", content="text/html;charset=UTF-8")
    meta(name="keywords", content="")
    meta(name="description", content="")
    link(href='http://fonts.googleapis.com/css?family=Lato:400,700,900', rel='stylesheet', type='text/css')
    link(rel="stylesheet", href="style.css")

body
  header.container.header
    .pure-g
      .pure-u-1.md-1-2
        h1
          span.wow.fadeInDown(class="titulo-1") jacobo
          span.wow.fadeInDown(class="titulo-2", data-wow-delay=".1s") rus
          span.wow.fadeInDown(class="titulo-1", data-wow-delay=".2s") .
          span.wow.fadeInDown(class="titulo-3", data-wow-delay=".3s") codes
      .pure-u-1.md-1-2.social
        a(href="http://github.com/jacoborus", target="blank", title="Github")
          img.wow.fadeInDown(src="img/github.svg", data-wow-delay=".4s")
        a.twitter(href="http://twitter.com/jacoborus", target="blank", title="Twitter")
          img.wow.fadeInDown(src="img/twitter.svg", data-wow-delay=".5s")

  section#projects.pure-g
    each project in projects
      article
        h1.logo= project.title
        p= project.description
        a(href=project.url, title=project.title, target="blank")= project.showUrl
