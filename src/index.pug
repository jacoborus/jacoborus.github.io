doctype html
html
  head
    meta(charset="utf-8")
    meta(name="viewport", content="width=device-width, initial-scale=1")

    title jacoborus.codes
    meta(http-equiv="Content-type", content="text/html;charset=UTF-8")
    meta(name="keywords", content="")
    meta(name="description", content="")
    link(href='https://fonts.googleapis.com/css?family=Lato:400,700,900', rel='stylesheet', type='text/css')
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/normalize/4.2.0/normalize.min.css")
    link(rel="stylesheet", href="css/style.css")

body
  header.header
    h1
      span(class="titulo-1") jacobo
      span(class="titulo-2") rus
      span(class="titulo-1") .
      span(class="titulo-2") codes
      a(class="social" href="https://github.com/jacoborus", target="blank", title="Github")
        img(src="img/github.svg")
      a.twitter(class="social" href="https://twitter.com/jacoborus", target="blank", title="Twitter")
        img(src="img/twitter.svg")

  section#projects.pure-g
    each project in projects
      article
        h1.logo= project.title
        p= project.description
        a(href=project.url, title=project.title, target="blank")= project.showUrl

